---
import Layout from './Layout.astro';

interface Props {
	title?: string;
	description?: string;
	ogImage?: string;
	ogType?: string;
	jsonLd?: string;
}

const { title, description, ogImage, ogType, jsonLd } = Astro.props;
---

<Layout title={title} description={description} ogImage={ogImage} ogType={ogType} jsonLd={jsonLd}>
	<slot />
</Layout> 